.aui .popup option, .aui .popup button{
    font-size: 15px;
}

.aui .popup button{
    padding: 4px 12px;
}

.aui address.municipipality p,  .aui div.horario p{
    line-height: 1.4em;
    margin: 0;
}

.aui .catalogo li {
    line-height: 1.4em;
}

.aui .oficina-vivienda .table-striped>tbody>tr:nth-child(odd)>td, .aui .table-striped>tbody>tr:nth-child(odd)>th {
    background-color: #fff; 
}

.aui .oficina-vivienda .table-hover>tbody>tr:hover>td, .aui .table-hover>tbody>tr:hover>th {
    background-color: #fff;
}

.aui .oficina-vivienda .table>tbody>tr>td, .aui .oficina-vivienda .table {
   border:0px;
   padding:0px;
}